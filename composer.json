{
    "autoload": {
        "psr-0": {
            "": [
                "vendor/studioforty/qa/src/app",
                "vendor/studioforty/qa/src/lib",
                "vendor/studioforty/qa/src/app/code/local",
                "vendor/studioforty/qa/src/app/code/community",
                "vendor/studioforty/qa/src/app/code/core"
            ],
            "StudioForty9": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "description": "StudioForty9 Quality Assurance module",
    "extra": {
        "map": [
            [
                "features/",
                "features/"
            ],
            [
                "behat.yml.sample",
                "behat.yml.sample"
            ]
        ]
    },
    "name": "studioforty9/qa",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        }
    ],
    "require": {
        "behat/mink-selenium2-driver": "*",
        "magento-ecg/coding-standard": "1.2",
        "magento-hackathon/magento-composer-installer": "*",
        "magetest/magento-behat-extension": "*",
        "phpunit/phpunit": "*"
    },
    "type": "magento-module",
    "version": "0.0.1"
}
