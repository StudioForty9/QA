{
    "autoload": {
        "psr-0": {
            "": [
                "./app",
                "./app/code/local",
                "./app/code/community",
                "./app/code/core",
                "./lib"
            ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "description": "StudioForty9 Quality Assurance module",
    "extra": {
        "map": [
            [
                "features/",
                "features/"
            ],
            [
                "behat.yml.sample",
                "behat.yml.sample"
            ]
        ]
    },
    "name": "studioforty9/qa",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        },
        {
            "type": "git",
            "url": "git@github.com:MageTest/BehatMage.git"
        }
    ],
    "require": {
        "behat/mink-selenium2-driver": "*",
        "magento-ecg/coding-standard": "1.2",
        "magento-hackathon/magento-composer-installer": "*",
        "magetest/magento-behat-extension": "dev-develop",
        "phpunit/phpunit": "*"
    },
    "type": "magento-module",
    "version": "0.0.1"
}